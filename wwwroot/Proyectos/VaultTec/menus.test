.button{
    border-radius: 50%;
    position: fixed;
    background: none;
    border: none;
    /*Creditos por la imagen: Timothy Miller - https://www.iconfinder.com/icons/134216/hamburger_lines_menu_icon, Public Domain, https://commons.wikimedia.org/w/index.php?curid=50764696*/

    background-repeat: no-repeat;
    position: absolute;
    z-index: 9999;
    border: 0;
    width: 1rem;
    aspect-ratio: 1;
    top: 2rem;
    right: 3rem;
    align-items:center; 
    max-height: 20vw;
    transform: translateX(50%);
    transition: transform 350ms ease-in;
}
