<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - P1_ASP</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/P1_ASP.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/MyStyleSheet.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-dark bg-dark navbar-expand-lg navbar-light bg-light text-center">
            <div class="container-fluid">
                <div class="collapse navbar-collapse">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link active neonText" asp-area="" asp-controller="Home" asp-action="Proyectos">Projects</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link active neonText" asp-area="" asp-controller="Home" asp-action="Index">About me..</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link active neonTextAnimated" asp-area="" asp-controller="Home" asp-action="Privacy">Contact</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link active neonText" asp-area="" asp-controller="Home" asp-action="Experience">Experience</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link active neonTextAnimated" asp-area="" asp-controller="Home" asp-action="AcademicExperience">Academic Training</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link active neonText" asp-area="" asp-controller="Home" asp-action="TechnicalKnowledge">Technical Knowledge</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link active neonText" asp-area="" asp-controller="Home" asp-action="CurriculumVitae">Curriculum Vitae</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container-fluid">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - P1_ASP - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
